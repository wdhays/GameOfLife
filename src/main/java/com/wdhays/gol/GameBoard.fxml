<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="stackPane" id="stackPane"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefWidth="600.0" prefHeight="650.0">
    <children>
        <GridPane fx:id="gridPane" id="gridPane" prefWidth="600.0" prefHeight="600.0"/>
        <HBox fx:id="gridInfo" id="gridInfo" prefWidth="600.0" prefHeight="25.0" alignment="CENTER">
            <VBox alignment="CENTER" prefWidth="150">
                <Label fx:id="gameSpeedLabel" id="gameSpeedLabel" text="Game Speed: "/>
                <Label fx:id="gameSpeedLabelValue" id="gameSpeedLabel" text=""/>
            </VBox>
            <VBox alignment="CENTER" prefWidth="150">
                <Label fx:id="gameStateLabel" id="gameStateLabel" text="Game State: "/>
                <Label fx:id="gameStateLabelValue" id="gameStateLabel" text=""/>
            </VBox>
            <VBox alignment="CENTER" prefWidth="150">
                <Label fx:id="generationLabel" id="generationLabel" text="Generation: "/>
                <Label fx:id="generationLabelValue" id="generationLabel" text=""/>
            </VBox>
        </HBox>
    </children>
</VBox>


